{
	"manifest_version": 3,
	"name": "YM WP Translator Tools",
	"description": "",
	"author": "Yan Metelitsa",
	"version": "1.0.0",
	"icons": {
		"128": "icons/icon-128-white.png"
	},
	"permissions": [
		"scripting"
	],
	"host_permissions": [
		"https://translate.wordpress.org/projects/*"
	],
	"action": {
		"default_icon": {
			"128": "icons/icon-128-white.png"
		},
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "js/background.js"
	}
}